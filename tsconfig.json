{
  "compilerOptions": {
    "target": "es5",
    
    "lib": [
      "dom",
      "dom.iterable",
      "esnext","es2015", "es2016", "es2017",
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "ts-node": {
    "esm": true
},
  "include": [
    ".dist/src/index.ts",
    ".dist/src/checkDisabledLinters.ts"
  ]
}